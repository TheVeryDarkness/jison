{
  "name": "ts-jison",
  "description": "A lightly-typescriptified version of jison",
  "homepage": "http://jison.org",
  "license": "MIT",
  "keywords": [
    "jison",
    "bison",
    "yacc",
    "parser",
    "generator",
    "lexer",
    "flex",
    "tokenizer",
    "compiler",
    "typescript"
  ],
  "private": false,
  "version": "0.0.6",
  "repository": {
    "type": "git",
    "url": "git://github.com/ericprud/ts-jison.git"
  },
  "bugs": {
    "url": "http://github.com/ericprud/ts-jison/issues"
  },
  "author": {
    "name": "Eric Prudhommeaux",
    "email": "eric@w3.org",
    "url": "http://www.w3.org/People/Eric/"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=0.4"
  },
  "dependencies": {
    "@types/node": "^14.14.25",
    "browserify": "^17.0.0",
    "nomnom": "1.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "uglify-js": "^3.13.3"
  },
  "devDependencies": {
    "@ts-jison/common": "file:packages/common",
    "@ts-jison/common-generator": "file:packages/common-generator",
    "@ts-jison/ebnf-parser": "file:packages/ebnf-parser",
    "@ts-jison/lexer": "file:packages/lexer",
    "@ts-jison/lexer-generator": "file:packages/lexer-generator",
    "@ts-jison/lex-parser": "file:packages/lex-parser",
    "@ts-jison/parser": "file:packages/parser",
    "@ts-jison/parser-generator": "file:packages/parser-generator",
    "jest": "^29.5.0",
    "jison": "git://github.com/zaach/jison.git#ef2647",
    "jison": "0.4.x",
    "lerna": "^6.5.1",
    "test": "0.6.x"
  },
  "scripts": {
    "build999": "lerna run build",
    "build": "(cd packages/common/ && tsc -b tsconfig.package.json) && (cd packages/lexer/ && tsc -b tsconfig.package.json) && (cd packages/parser/ && tsc -b tsconfig.package.json)",
    "clean": "rm -r packages/{common,lexer,parser}/lib/",
    "js-calculator-demo": "(cd packages/parser-generator/ && npm run js-calculator-demo)",
    "ts-calculator-demo": "(cd packages/parser-generator/ && npm run ts-calculator-demo)",
    "ts-node-calculator-demo": "(cd packages/parser-generator/ && npm run ts-node-calculator-demo)",
    "test": "npm run js-calculator-demo && npm run ts-calculator-demo && npm run ts-node-calculator-demo && (cd packages/lexer-generator/ && node tests/all-tests.js)",
    "test:make": "(cd packages/parser-generator/examples/ && make clean && make js-calculator-demo && make ts-calculator-demo && make ts-node-calculator-demo)",
    "test-todo": "(cd packages/parser-generator/ && node tests/all-tests.js);"
  }
}
